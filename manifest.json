{
  "manifest_version": 2,

  "name": "Hide Facebook Ads",
  "description": "This extension hides adds on Facebook stream",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["fb-adblock.js"]
    }
  ],
  "page_action": {
    "default_icon": "icon.png"
  }
}
